{
  "name": "conduit-node",
  "version": "1.0.0",
  "description": "conduit on node",
  "main": "app.js",
  "scripts": {
    "start": "node ./dist",
    "dev": "nodemon ./src --exec babel-node ./src",
    "prebuild": "rimraf dist",
    "generateData": "babel-node scripts/generate",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js,__mocks__ src",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "license": "ISC",
  "repository": "git@github.com:kentcdodds/testing-workshop.git",
  "dependencies": {
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "errorhandler": "^1.5.0",
    "express": "^4.14.1",
    "express-jwt": "^5.1.0",
    "express-session": "^1.15.1",
    "jsonwebtoken": "^7.3.0",
    "method-override": "^2.3.7",
    "methods": "1.1.2",
    "mongoose": "^4.8.4",
    "mongoose-unique-validator": "^1.0.3",
    "morgan": "^1.8.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "request": "^2.79.0",
    "slug": "0.9.1",
    "underscore": "1.8.3"
  },
  "author": "Thinkster (https://github.com/gothinkster)",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^1.1.3",
    "faker": "^4.1.0",
    "jest": "^19.0.2",
    "jest-matchers": "^19.0.0",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "setupTestFrameworkScriptFile": "./other/jest/expect-extensions.js",
    "collectCoverageFrom": [
      "**/config/**",
      "**/models/**",
      "**/routes/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0.5,
        "branches": 3,
        "functions": 1,
        "lines": 0.5
      }
    }
  }
}
