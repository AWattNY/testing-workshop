{
  "name": "react-redux-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-app": "^2.1.0",
    "cross-env": "^3.1.4",
    "jest": "^18.0.0",
    "nps": "^5.0.4",
    "nps-utils": "^1.2.0",
    "pushstate-server": "^2.2.1",
    "react-scripts": "^0.9.0"
  },
  "license": "MIT",
  "repository": "git@github.com:kentcdodds/testing-workshop.git",
  "dependencies": {
    "history": "^4.3.0",
    "marked": "^0.3.6",
    "query-string": "^4.3.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "superagent": "^3.5.0",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "postinstall": "nps postinstall"
  },
  "author": "Thinkster (https://github.com/gothinkster)",
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.jsx$"
    ],
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  }
}
